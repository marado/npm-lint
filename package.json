{
  "name": "npm-lint",
  "version": "0.3.1",
  "preferGlobal": true,
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "description": "A configurable package.json linter with a focus on security",
  "main": "bin/index.js",
  "scripts": {
    "lint": "node src/index.js",
    "test": "webpack && ava",
    "build": "webpack",
    "prepublishOnly": "npm run lint && npm run build",
    "watch": "webpack --watch"
  },
  "bin": {
    "npm-lint": "bin/index.js"
  },
  "engines": {
    "node": ">=v6.11.1"
  },
  "files": [
    "bin/**/*",
    "package-lock.json",
    ".npmlint.json",
    "README.md"
  ],
  "ava": {
    "files": [
      "test/**/*-spec.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tanepiper/package-linter.git"
  },
  "author": "Tane Piper <piper.tane@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "cli-table": "^0.3.1",
    "colors": "^1.1.2",
    "eslint": "^4.2.0",
    "lokijs": "^1.5.0",
    "npm": "^5.2.0",
    "npm-check-updates": "^2.12.1",
    "semver": "^5.3.0",
    "semver-regex": "^1.0.0",
    "source-map-support": "^0.4.15",
    "util.promisify": "^1.0.0",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@types/colors": "^1.1.3",
    "@types/node": "^8.0.12",
    "@types/yargs": "^8.0.1",
    "ava": "^0.21.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "nodemon": "^1.11.0",
    "sinon": "^2.3.8",
    "ts-loader": "^2.2.2",
    "typescript": "^2.4.1",
    "webpack": "^3.2.0",
    "webpack-node-externals": "^1.6.0"
  },
  "npmLint": {}
}
