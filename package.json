{
  "name": "npm-lint",
  "version": "0.2.7",
  "preferGlobal": true,
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "description": "A configurable package.json linter with a focus on security",
  "main": "bin/index.js",
  "scripts": {
    "lint": "node src/index.js",
    "test": "ava",
    "build": "webpack",
    "prepublishOnly": "npm run lint && npm run build",
    "watch": "webpack --watch"
  },
  "bin": {
    "npm-lint": "bin/index.js"
  },
  "files": [
    "bin/**/*",
    "package-lock.json",
    "README.md"
  ],
  "ava": {
    "files": [
      "test/**/*-spec.js"
    ],
    "source": [
      "src/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tanepiper/package-linter.git"
  },
  "author": "Tane Piper <piper.tane@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "cli-table": "^0.3.1",
    "colors": "^1.1.2",
    "eslint": "^4.2.0",
    "lokijs": "^1.5.0",
    "npm": "^5.2.0",
    "npm-check-updates": "^2.12.1",
    "semver-regex": "^1.0.0",
    "source-map-support": "^0.4.15"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "nodemon": "^1.11.0",
    "sinon": "^2.3.8",
    "webpack": "^3.2.0"
  },
  "npmLint": {}
}
